<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.matisotee.freestylejury">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_fj"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_round_fj"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ConfigurationActivity" />
        <activity android:name=".activities.BattleActivity" />
        <activity android:name=".activities.PuntuationActivity"></activity>
    </application>

</manifest>